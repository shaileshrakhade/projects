<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Customer_Profile</servlet-name>
        <servlet-class>carrent.Customer_Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sdr</servlet-name>
        <servlet-class>sdr</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>carrent.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newcar</servlet-name>
        <servlet-class>carrent.newcar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Customer_Profile</servlet-name>
        <url-pattern>/Customer_Profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sdr</servlet-name>
        <url-pattern>/sdr</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newcar</servlet-name>
        <url-pattern>/newcar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
